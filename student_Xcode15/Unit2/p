func progressUpdate(steps: Int, goal: Int) {
    if steps < goal * 0.1 {
        print("You're off to a good start.")
    } else if steps < goal / 2 {
        print("You're almost halfway there!")
    } else if steps < goal * 0.9 {
        print("You're over halfway there!")
    } else if steps < goal {
        print("You're almost there!")
    } else {
        print("You beat your goal!")
    }
}

// Call the progressUpdate function with different values
progressUpdate(steps: 500, goal: 10000)  // You're off to a good start.
progressUpdate(steps: 4000, goal: 10000) // You're almost halfway there!
progressUpdate(steps: 6000, goal: 10000) // You're over halfway there!
progressUpdate(steps: 9000, goal: 10000) // You're almost there!
progressUpdate(steps: 10000, goal: 10000) // You beat your goal!
