let alphabet = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"

// Loop through the alphabet and print every other letter
for index in alphabet.indices {
    let position = alphabet.distance(from: alphabet.startIndex, to: index)
    if position.isMultiple(of: 2) { // Print only letters at even indices
        print(alphabet[index])
    }
}

// Create a dictionary of states and capitals
let stateCapitals: [String: String] = [
    "California": "Sacramento",
    "Texas": "Austin",
    "New York": "Albany",
    "Florida": "Tallahassee" 
]

let homeState = "Florida"

// Loop through the dictionary and print keys and values
for (state, capital) in stateCapitals {
    print("\(state)'s capital is \(capital).")
    
    // Check if the current state is the home state
    if state == homeState {
        print("I found my home!")
        break // Exit the loop when home state is found
    }
}

